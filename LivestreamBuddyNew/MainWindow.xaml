<Window x:Class="LivestreamBuddyNew.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:LivestreamBuddyNew.Controls"
        Title="Livestream Buddy" Height="425" Width="525" Icon="livestream-ICON.ico">
    <Border Padding="13 13 13 13">
        <StackPanel Orientation="Vertical">
            <TabControl Name="mainTabs">
                <TabItem Header="Channels">
                    <controls:Channels x:Name="channelsControl"></controls:Channels>
                </TabItem>
            </TabControl>
        </StackPanel>
    </Border>
</Window>
